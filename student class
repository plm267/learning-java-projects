class Student extends Person {
    String program;    
    int year;
    double fee;
    Student(String name, String address, String program, int year, int fee){
    super(name, address);
    this.program = program; 
    this.year = year;
    this.fee = fee;
    }
public String getProgram(){
        return program;}

   public void setProgram(String program) {
      this.program = program;
   }
   public int getyear() {
      return year;
   }
   public void setYear(int year) {
      this.year = year;
   }
      public double getFee() {
      return fee;
   }
   public void setFee(double fee) {
      this.fee = fee;
   }
public String toString() {
      return "Student[" + super.toString()+", "+program+", "+year+", "+fee+"0]";
   }

}

Student s = new Student("Jimmy", "123 Somewhere", "Information Technology", 2016, 500);
System.out.println(s);
